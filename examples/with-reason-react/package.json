{
  "name": "razzle-examples-reason-react",
  "version": "0.7.6-rc2",
  "license": "MIT",
  "scripts": {
    "start": "npm run js-watch | npm run bsb-watch",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w",
