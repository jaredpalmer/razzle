    "build": "bsb -make-world && razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "reason-react": "^0.1.5"
  },
  "devDependencies": {
    "bs-platform": "latest",
    "razzle": "^0.7.6-rc2"
  }
}
