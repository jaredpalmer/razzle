{
  "name": "razzle-examples-with-reason-react",
  "version": "4.2.15",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "^0.8.0"
  },
  "devDependencies": {
    "bs-platform": "7.1.1",
    "@glennsl/bs-json": "^4.0.0",
    "bs-fetch": "^0.5.0",
    "bs-webapi": "^0.15.2",
    "concurrently": "^5.2.0",
    "razzle": "4.2.15",
    "razzle-dev-utils": "4.2.15",
    "mini-css-extract-plugin": "^0.9.0",
    "html-webpack-plugin": "^4.5.2",
    "webpack": "^4.44.1",
    "babel-preset-razzle": "4.2.15",
    "webpack-dev-server": "^3.11.2"
  }
}
